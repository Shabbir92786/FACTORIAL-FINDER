    "#mini project : factorial finder using PYTHON \n",
    "\n",
    "a = int(input(\"write numer to find factorial\"))\n",
    "\n",
    "def fact(a):\n",
    "    if(a==0):\n",
    "        result=1\n",
    "    else:\n",
    "        result=a*fact(a-1)\n",
    "    return result\n",
    "print(fact(a))"
   ]
